#, c-format
msgid "Login as %s"
msgstr "Daftar masuk sebagai %s"

#: ../src/user-list.vala:790
msgid "Retry"
msgstr "Cuba lagi"

#: ../src/user-list.vala:791
#, c-format
msgid "Retry as %s"
msgstr "Cuba sekali lagi sebagai %s"

